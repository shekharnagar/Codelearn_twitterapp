package com.kreate.twitter.models;


/**
 * Created by Nagar on 9/26/2015.
 */
public class Tweet {

    private String id;
    private static String title;
    private static String body;

    public static String getTitle() {
        return title;
    }

    public void setTitle(String title) {
        this.title = title;
    }

    public static String getBody() {
        return body;
    }

    public void setBody(String body) {
        this.body = body;
    }

    public String getId() {
        return this.id;
    }

}
